#include "RLMSwiftPropertyTypes.h"

import Realm
import Realm.Private

// clang -x c -E -CC -P -I Realm RealmSwift/ManagedPropertyTypes.swift.in >! RealmSwift/ManagedPropertyTypes.swift

#define REALM_VALUE_EXTENSION(objc, swift, rlmtype) /*
   */ extension swift { /*
       */ @inlinable /*
       */ public static func _rlmSetProperty(_ obj: Object, _ key: UInt16, _ value: swift) { /*
           */ RLMSetSwiftProperty##swift(obj, key, value) /*
       */ } /*
       */ @inlinable public static var _type: PropertyType { .rlmtype } /*
       */ @inlinable public static var _optional: Bool { false } /*
   */ }
REALM_FOR_EACH_SWIFT_PRIMITIVE_TYPE(REALM_VALUE_EXTENSION)
REALM_FOR_EACH_SWIFT_OBJECT_TYPE(REALM_VALUE_EXTENSION)
#undef REALM_VALUE_EXTENSION

#define REALM_VALUE_EXTENSION(objc, swift, rlmtype) /*
 */ extension swift: RealmValue { /*
     */ @inlinable /*
     */ public static func _rlmGetProperty(_ obj: Object, _ key: UInt16) -> swift { /*
         */ return RLMGetSwiftProperty##swift(obj, key) /*
     */ } /*
     */ @inlinable /*
     */ public static func _rlmGetPropertyOptional(_ obj: Object, _ key: UInt16) -> swift? { /*
         */ var gotValue = false /*
         */ let ret = RLMGetSwiftProperty##swift##Optional(obj, key, &gotValue) /*
         */ return gotValue ? ret : nil /*
     */ } /*
 */ }

REALM_FOR_EACH_SWIFT_PRIMITIVE_TYPE(REALM_VALUE_EXTENSION)
#undef REALM_VALUE_EXTENSION

#define REALM_VALUE_EXTENSION(objc, swift, rlmtype) /*
 */ extension swift: RealmValue { /*
     */ @inlinable /*
     */ public static func _rlmGetProperty(_ obj: Object, _ key: UInt16) -> swift { /*
         */ return RLMGetSwiftProperty##swift(obj, key)! /*
     */ } /*
     */ @inlinable /*
     */ public static func _rlmGetPropertyOptional(_ obj: Object, _ key: UInt16) -> swift? { /*
         */ return RLMGetSwiftProperty##swift(obj, key) /*
     */ } /*
 */ }

REALM_FOR_EACH_SWIFT_OBJECT_TYPE(REALM_VALUE_EXTENSION)
#undef REALM_VALUE_EXTENSION

